    "abacus-moment": "file:../../lib/utils/moment",
    "abacus-request": "file:../../lib/utils/request",
    "commander": "^2.8.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "abacus-eslint": "file:../../tools/eslint",
    "abacus-publish": "file:../../tools/publish"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "src/test/**"
    ]
