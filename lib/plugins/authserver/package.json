{
  "name": "abacus-authserver-plugin",
  "description": "Plugin for an authrorization server",
  "license": "Apache-2.0",
  "version": "1.1.3",
  "private": true,
  "homepage": "https://github.com/cloudfoundry-incubator/cf-abacus/tree/master/lib/plugins/authserver",
  "bugs": {
    "url": "https://github.com/cloudfoundry-incubator/cf-abacus/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/cloudfoundry-incubator/cf-abacus.git"
  },
  "keywords": [
    "cf",
    "abacus"
  ],
  "files": [
    ".npmrc",
    "manifest.yml",
    "src/"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js &",
    "stop": "pkill -f 'node abacus-authserver-plugin-[0-9]+-[0-9]+ express' || true",
    "test": "abacus-eslint && nyc abacus-mocha",
    "generate-coverage": "nyc report --reporter=lcovonly",
    "lint": "abacus-eslint",
    "pub": "publish",
    "cfpush": "cfpush"
  },
  "config": {
    "port": 9882
  },
  "dependencies": {
    "abacus-debug": "file:../../utils/debug",
    "abacus-router": "file:../../utils/router",
    "abacus-webapp": "file:../../utils/webapp",
    "underscore": "^1.9.1",
    "jsonwebtoken": "^8.3.0",
    "ajv": "^6.0.0"
  },
  "devDependencies": {
    "abacus-cfpush": "file:../../../tools/cfpush",
    "abacus-eslint": "file:../../../tools/eslint",
    "abacus-mocha": "file:../../../tools/mocha",
    "nyc": "^13.0.1",
    "abacus-publish": "file:../../../tools/publish"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "src/test/**"
    ]
  },
  "engines": {
    "node": ">=8.10.0 <9.0.0",
    "npm": "<5.0.0",
    "yarn": ">=1.2.1"
  }
}
