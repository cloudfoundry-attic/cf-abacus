{
  "name": "abacus-provisioning-plugin",
  "description": "Plugin for a provisioning history service",
  "license": "Apache-2.0",
  "version": "1.1.0",
  "private": true,
  "homepage": "https://github.com/cloudfoundry-incubator/cf-abacus-broker/lib/plugins/provisioning",
  "bugs": {
    "url": "https://github.com/cloudfoundry-incubator/cf-abacus-broker/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/cloudfoundry-incubator/cf-abacus-broker.git"
  },
  "keywords": [
    "cf",
    "abacus",
    "broker"
  ],
  "files": [
    ".apprc",
    ".npmrc",
    "manifest.yml",
    "src/"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "launch webapp start",
    "stop": "webapp stop",
    "test": "abacus-eslint && nyc abacus-mocha",
    "generate-coverage": "nyc report --reporter=lcovonly",
    "lint": "abacus-eslint",
    "pub": "publish",
    "cfpush": "cfpush",
    "store-defaults": "node src/tasks/store-default-mappings.js && node src/tasks/store-default-plans.js"
  },
  "dependencies": {
    "abacus-batch": "file:../../utils/batch",
    "abacus-breaker": "file:../../utils/breaker",
    "abacus-dbclient": "file:../../utils/dbclient",
    "abacus-debug": "file:../../utils/debug",
    "abacus-launcher": "file:../../utils/launcher",
    "abacus-lock": "file:../../utils/lock",
    "abacus-lrucache": "file:../../utils/lrucache",
    "abacus-oauth": "file:../../utils/oauth",
    "abacus-partition": "file:../../utils/partition",
    "abacus-plan-mappings": "file:../../config/mappings",
    "abacus-provisioning-plugin": "file:../../plugins/provisioning",
    "abacus-request": "file:../../utils/request",
    "abacus-retry": "file:../../utils/retry",
    "abacus-router": "file:../../utils/router",
    "abacus-urienv": "file:../../utils/urienv",
    "abacus-usage-schemas": "file:../../config/schemas",
    "abacus-webapp": "file:../../utils/webapp",
    "abacus-yieldable": "file:../../utils/yieldable",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "abacus-cfpush": "file:../../../tools/cfpush",
    "abacus-cluster": "file:../../utils/cluster",
    "abacus-eslint": "file:../../../tools/eslint",
    "abacus-mocha": "file:../../../tools/mocha",
    "nyc": "^11.4.1",
    "abacus-publish": "file:../../../tools/publish",
    "jsonwebtoken": "^5.0.5"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "src/test/**"
    ]
  },
  "engines": {
    "node": ">=8.9.4 <9.0.0",
    "npm": "<5.0.0",
    "yarn": ">=1.2.1"
  }
}
