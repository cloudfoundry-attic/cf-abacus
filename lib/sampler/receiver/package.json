{
  "name": "abacus-sampler-receiver",
  "description": "Receiver component for Abacus Sampler",
  "license": "Apache-2.0",
  "version": "1.1.5-rc.0",
  "private": true,
  "homepage": "https://github.com/cloudfoundry-incubator/cf-abacus/tree/master/lib/sampler/receiver",
  "bugs": {
    "url": "https://github.com/cloudfoundry-incubator/cf-abacus/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/cloudfoundry-incubator/cf-abacus.git"
  },
  "keywords": [
    "abacus",
    "sampler",
    "receiver"
  ],
  "files": [
    ".npmrc",
    "src/"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "launch &",
    "stop": "pkill -f 'node abacus-sampler-receiver-[0-9]+ express' || true",
    "test": "abacus-eslint && nyc abacus-mocha",
    "generate-coverage": "nyc report --reporter=lcovonly",
    "lint": "abacus-eslint",
    "pub": "publish",
    "cfpush": "cfpush"
  },
  "config": {
    "port": 7070
  },
  "dependencies": {
    "abacus-api-clients": "file:../../utils/api",
    "abacus-debug": "file:../../utils/debug",
    "abacus-launcher": "file:../../utils/launcher",
    "abacus-healthmonitor": "file:../../utils/healthmonitor",
    "abacus-moment": "file:../../utils/moment",
    "abacus-oauth": "file:../../utils/oauth",
    "abacus-router": "file:../../utils/router",
    "abacus-sampler-storage": "file:../storage",
    "abacus-schema": "file:../../utils/schema",
    "abacus-webapp": "file:../../utils/webapp",
    "ajv": "^6.0.0",
    "http-status-codes": "^1.3.0",
    "is-my-json-valid": "^2.12.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "abacus-cfpush": "file:../../../tools/cfpush",
    "abacus-eslint": "file:../../../tools/eslint",
    "abacus-mocha": "file:../../../tools/mocha",
    "abacus-publish": "file:../../../tools/publish",
    "nyc": "^13.0.1"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "src/test/**"
    ]
  },
  "engines": {
    "node": ">=8.10.0 <9.0.0",
    "npm": "<5.0.0",
    "yarn": ">=1.2.1"
  }
}
