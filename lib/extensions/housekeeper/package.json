{
  "name": "abacus-housekeeper",
  "description": "Abacus Housekeeper",
  "license": "Apache-2.0",
  "version": "1.1.3",
  "private": true,
  "homepage": "https://github.com/cloudfoundry-incubator/cf-abacus/tree/master/lib/extensions/housekeeper",
  "bugs": {
    "url": "https://github.com/cloudfoundry-incubator/cf-abacus/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/cloudfoundry-incubator/cf-abacus.git"
  },
  "keywords": [
    "cf",
    "abacus",
    "broker"
  ],
  "files": [
    ".npmrc",
    "manifest.yml",
    "src/"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js &",
    "stop": "pkill -f 'node abacus-housekeeper-[0-9]+-[0-9]+ express' || true",
    "test": "abacus-eslint && nyc abacus-mocha",
    "generate-coverage": "nyc report --reporter=lcovonly",
    "lint": "abacus-eslint",
    "pub": "publish",
    "cfpush": "cfpush"
  },
  "config": {
    "port": 9883
  },
  "dependencies": {
    "abacus-client": "file:../../utils/client",
    "abacus-debug": "file:../../utils/debug",
    "abacus-dbclient": "file:../../utils/dbclient",
    "abacus-oauth": "file:../../utils/oauth",
    "abacus-request": "file:../../utils/request",
    "abacus-router": "file:../../utils/router",
    "abacus-throttle": "file:../../utils/throttle",
    "abacus-transform": "file:../../utils/transform",
    "abacus-urienv": "file:../../utils/urienv",
    "abacus-webapp": "file:../../utils/webapp",
    "abacus-moment": "file:../../utils/moment",
    "jsonwebtoken": "^8.3.0",
    "underscore": "^1.9.1",
    "ajv": "^6.0.0"
  },
  "devDependencies": {
    "abacus-cfpush": "file:../../../tools/cfpush",
    "abacus-eslint": "file:../../../tools/eslint",
    "abacus-mocha": "file:../../../tools/mocha",
    "nyc": "^13.0.1",
    "abacus-publish": "file:../../../tools/publish"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "src/test/**"
    ]
  },
  "engines": {
    "node": ">=8.10.0 <9.0.0",
    "npm": "<5.0.0",
    "yarn": ">=1.2.1"
  }
}
