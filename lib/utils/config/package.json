{
  "name": "abacus-config",
  "description": "Exports abcus configurations",
  "license": "Apache-2.0",
  "version": "1.1.5",
  "private": true,
  "homepage": "https://github.com/cloudfoundry-incubator/cf-abacus/tree/master/lib/utils/config",
