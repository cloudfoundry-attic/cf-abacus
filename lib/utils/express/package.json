{
  "name": "abacus-express",
  "description": "Setup Express with a selection of useful Express middleware",
  "license": "Apache-2.0",
  "version": "1.1.3",
  "private": true,
  "homepage": "https://github.com/cloudfoundry-incubator/cf-abacus/lib/utils/express",
  "bugs": {
    "url": "https://github.com/cloudfoundry-incubator/cf-abacus/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/cloudfoundry-incubator/cf-abacus.git"
  },
  "keywords": [
    "cf",
    "abacus"
  ],
  "files": [
    ".npmrc",
    "src/"
  ],
  "main": "src/index.js",
  "scripts": {
    "test": "abacus-eslint && nyc abacus-mocha",
    "generate-coverage": "nyc report --reporter=lcovonly",
    "lint": "abacus-eslint",
    "pub": "publish"
  },
  "dependencies": {
    "abacus-debug": "file:../debug",
    "abacus-events": "file:../events",
    "abacus-moment": "file:../moment",
    "abacus-oauth": "file:../oauth",
    "abacus-urienv": "file:../urienv",
    "abacus-vcapenv": "file:../vcapenv",
    "body-parser": "1.18.3",
    "compression": "^1.7.2",
    "express": "4.16.3",
    "express-rate-limit": "^3.1.1",
    "http-status-codes": "^1.3.0",
    "method-override": "^3.0.0",
    "mongodb": "^3.1.6",
    "morgan": "^1.1.1",
    "response-time": "^2.3.2",
    "serve-static": "^1.13.2",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "async": "^2.6.0",
    "abacus-eslint": "file:../../../tools/eslint",
    "abacus-mocha": "file:../../../tools/mocha",
    "abacus-publish": "file:../../../tools/publish",
    "abacus-request": "file:../request",
    "jsonwebtoken": "^8.3.0",
    "nyc": "^13.0.1",
    "supertest": "^3.0.0"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "src/test/**"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "mongodb"
    ]
  },
  "engines": {
    "node": ">=8.9.4 <9.0.0",
    "npm": "<5.0.0",
    "yarn": ">=1.2.1"
  }
}
