{
  "name": "abacus-dataflow",
  "description": "Simple and configurable map and reduce dataflow transforms",
  "license": "Apache-2.0",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/cloudfoundry-incubator/cf-abacus/lib/utils/dataflow",
  "bugs": {
