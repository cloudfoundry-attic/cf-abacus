{
  "name": "abacus-etc",
  "description": "Misc Abacus build scripts",
  "license": "Apache-2.0",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/cloudfoundry-incubator/cf-abacus/etc",
  "bugs": {
    "url": "https://github.com/cloudfoundry-incubator/cf-abacus/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/cloudfoundry-incubator/cf-abacus.git"
  },
  "keywords": [
    "cf",
    "abacus"
  ],
  "files": [
    ".npmrc",
    "appname",
    "apprc",
    "apps.rc",
    "foreach",
    "secgroup.json",
    "versions",
    "src/"
  ],
  "main": "src/index.js",
  "bin": {
    "apprc": "./apprc",
    "foreach": "./foreach"
  },
  "scripts": {
    "test": "eslint",
    "report-coverage": "true",
    "lint": "eslint",
    "pub": "publish"
  },
  "dependencies": {
    "commander": "^2.8.1",
    "js-yaml": "^3.4.3",
    "strip-json-comments": "0.1.x",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "abacus-eslint": "file:../tools/eslint",
    "abacus-publish": "file:../tools/publish"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "src/test/**"
    ]
  },
  "engines": {
    "node": ">=6.11.4 <9.0.0",
    "npm": "<5.0.0"
  }
}
