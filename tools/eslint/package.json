{
  "name": "abacus-eslint",
  "description": "ESLint configuration and build script.",
  "license": "Apache-2.0",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/cloudfoundry-incubator/cf-abacus/tools/eslint",
  "bugs": {
    "url": "https://github.com/cloudfoundry-incubator/cf-abacus/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/cloudfoundry-incubator/cf-abacus.git"
  },
  "keywords": [
    "cf",
    "abacus"
  ],
  "files": [
    ".eslintrc",
    "abacus-eslint",
    ".npmrc",
    "src/"
  ],
  "main": "src/index.js",
  "bin": {
    "abacus-eslint": "./abacus-eslint"
  },
  "scripts": {
    "lint": "eslint -f stylish -c .eslintrc src",
    "test": "abacus-eslint -f stylish -c .eslintrc src",
    "generate-coverage": "true",
    "pub": "publish"
  },
  "dependencies": {
    "commander": "^2.8.1",
    "eslint": "~4.13.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-nodate": "file:../eslint-plugin-nodate",
    "optionator": "^0.6.0"
  },
  "devDependencies": {
    "abacus-publish": "file:../publish"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "src/test/**"
    ]
  },
  "engines": {
    "node": ">=8.9.4 <9.0.0",
    "npm": "<5.0.0"
  }
}
