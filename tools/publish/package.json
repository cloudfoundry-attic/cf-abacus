{
  "name": "abacus-publish",
  "description": "Npm publish build script",
  "license": "Apache-2.0",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/cloudfoundry-incubator/cf-abacus/tools/publish",
  "bugs": {
    "url": "https://github.com/cloudfoundry-incubator/cf-abacus/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/cloudfoundry-incubator/cf-abacus.git"
  },
  "keywords": [
    "cf",
    "abacus"
  ],
  "files": [
    ".npmrc",
    "publish",
    "src/"
  ],
  "main": "src/index.js",
  "bin": {
    "publish": "./publish"
  },
  "scripts": {
    "test": "abacus-eslint",
    "generate-coverage": "true",
    "lint": "abacus-eslint",
    "pub": "npm install && ./publish"
  },
  "dependencies": {
    "commander": "^2.8.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "abacus-eslint": "file:../eslint",
    "abacus-mocha": "file:../mocha"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "src/test/**"
    ]
  },
  "engines": {
    "node": ">=8.9.4 <9.0.0",
    "npm": "<5.0.0"
  }
}
