{
  "name": "abacus-cfpack",
  "description": "CloudFoundry app package build script",
  "license": "Apache-2.0",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/cloudfoundry-incubator/cf-abacus/tools/cfpack",
  "bugs": {
    "url": "https://github.com/cloudfoundry-incubator/cf-abacus/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/cloudfoundry-incubator/cf-abacus.git"
  },
  "keywords": [
    "cf",
    "abacus"
  ],
  "files": [
    ".npmrc",
    "cfpack",
    "src/"
  ],
  "main": "src/index.js",
  "bin": {
    "cfpack": "./cfpack"
  },
  "scripts": {
    "test": "eslint && nyc mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint",
    "pub": "publish"
  },
  "dependencies": {
    "commander": "^2.8.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "abacus-eslint": "file:../eslint",
    "abacus-mocha": "file:../mocha",
    "abacus-publish": "file:../publish",
    "rimraf": "^2.6.1"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "src/test/**"
    ]
  },
  "engines": {
    "node": ">=6.11.4 <9.0.0",
    "npm": "<5.0.0"
  }
}
