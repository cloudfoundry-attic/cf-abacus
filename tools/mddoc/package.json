{
  "name": "abacus-mddoc",
  "description": "Markdown to HTML doc build script",
  "license": "Apache-2.0",
  "version": "1.1.4-rc.3",
  "private": true,
  "homepage": "https://github.com/cloudfoundry-incubator/cf-abacus/tree/master/tools/mddoc",
  "bugs": {
    "url": "https://github.com/cloudfoundry-incubator/cf-abacus/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/cloudfoundry-incubator/cf-abacus.git"
  },
  "keywords": [
    "cf",
    "abacus"
  ],
  "files": [
    ".npmrc",
    "mddoc",
    "css/",
    "html/",
    "src/"
  ],
  "main": "src/index.js",
  "bin": {
    "mddoc": "./mddoc"
  },
  "scripts": {
    "test": "abacus-eslint && nyc abacus-mocha",
    "generate-coverage": "nyc report --reporter=lcovonly",
    "lint": "abacus-eslint",
    "pub": "publish"
  },
  "dependencies": {
    "bootstrap": "4.2.1",
    "commander": "^2.8.1",
    "event-stream": "3.3.4",
    "highlight.js": "^9.12.0",
    "hogan.js": "^3.0.2",
    "html-inline": "^1.1.1",
    "markdown-it": "^8.4.2",
    "path-is-absolute": "^2.0.0",
    "request": "2.88.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "abacus-eslint": "file:../eslint",
    "abacus-mocha": "file:../mocha",
    "nyc": "^13.0.1",
    "abacus-publish": "file:../publish"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "src/test/**"
    ]
  },
  "engines": {
    "node": ">=8.10.0 <9.0.0",
    "npm": "<5.0.0",
    "yarn": ">=1.2.1"
  }
}
