#!/bin/bash

# Configure the Abacus env to use a local test CouchDB server (which you
# must have set up and started separately)

# Run this script with . ./localcouchdb to configure your shell env
# before running Abacus

export DBCLIENT=abacus-couchclient
export DB=http://localhost:5984

export RABBIT_URI=amqp://localhost:5672

export JOBS=1
